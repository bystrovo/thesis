@article{Eich1989,
abstract = {Events that originate through internal mental operations such as reasoning, imagination, and thought may be more colored by or connected to one's current mood than are those that emanate from external sources. If so, then a shift in mood state, between the occasions of event encoding and event retrieval, should have a greater adverse impact on one's memory for internal than for external events. To investigate this inference, a series of studies was conducted that relied on a continuous music technique to modify mood, and on the generate/read procedures devised by N. J. Slamecka and P. Graf (see record 1980-20399-001) to distinguish internal from external events. Considered collectively, the results suggest that internal events are less likely than external events to be recalled after a shift in mood state. Discussion centers on both the empirical limitations and theoretical implications of the present result, as well as on prospects for future research.},
author = {Eich, Eric and Metcalfe, Janet},
doi = {10.1037/0278-7393.15.3.443},
file = {:Users/oleg/Documents/Mendeley Desktop/Eich, Metcalfe - 1989 - Mood dependent memory for internal versus external events.pdf:pdf},
isbn = {0278-7393},
issn = {0278-7393},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
number = {3},
pages = {443--455},
title = {{Mood dependent memory for internal versus external events.}},
volume = {15},
year = {1989}
}
