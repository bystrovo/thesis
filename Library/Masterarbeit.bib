file = {:Users/oleg/Documents/Mendeley Desktop/Ghergulescu, Muntean - 2014 - A novel sensor-based methodology for learner's motivation analysis in game-based learning.pdf:pdf},
issn = {09535438},
journal = {Interacting with Computers},
keywords = {cognitive science,electroencephalography,game-based learning,motivation,motivation assessment,novel interaction paradigms,user studies},
mendeley-tags = {motivation},
number = {4},
pages = {305--320},
title = {{A novel sensor-based methodology for learner's motivation analysis in game-based learning}},
