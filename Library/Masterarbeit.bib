journal = {PhD Thesis: Gothenburg studies in Informatics},
keywords = {Human-computer interaction,aesthetics.,design research,experimental design,interaction design,ubiquitous computing},
number = {May},
pages = {244},
title = {{Designing everyday computational things}},
year = {2001}
}
